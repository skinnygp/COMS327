#include <stdio.h>
#include <stdlib.h>/* rand() adn srand() */
#include <time.h> /* time() */


int main(int argc, char *argv[])
{
int seed;

if(argc == 2){
 seed = atoi(argv[1]);
}
else{
 seed = time(NULL);
}
printf("Seed is %d\n", seed);
srand(seed); /*seed*/ 


printf("%d\n", (signed) rand() % 201 - 100);
return 0;
})


